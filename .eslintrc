{
  "env": {
    "node": true
  },
  "globals": {
    "afterEach": true,
    "beforeEach": true,
    "console": true,
    "describe": true,
    "expect": true,
    "it": true,
    "xit": true
  },
  "rules": {
    "block-scoped-var": 2,
    "brace-style": [2, "1tbs"],
    "consistent-return": 2,
    "consistent-this": [0, "self"],
    "curly": 2,
    "eol-last": 0,
    "no-else-return": 2,
    "no-eq-null": 2,
    "no-extra-parens": [2, "functions"],
    "no-floating-decimal": 2,
    "no-lonely-if": 2,
    "no-mixed-spaces-and-tabs": [2, true],
    "no-nested-ternary": 2,
    "no-path-concat": 2,
    "no-redeclare": 2,
    "no-self-compare": 2,
    "semi-spacing": 2,
    "no-trailing-spaces": 2,
    "no-undefined": 0,
    "no-underscore-dangle": 0,
    "no-unused-vars": [2, {
      "args": "none",
      "vars": "all"
    }],
    "no-use-before-define": [2,"nofunc"],
    "quotes": [1, "double", "avoid-escape"],
    "radix": 2,
    "semi": [2, "always"],
    "sort-vars": 2,
    "space-after-keywords": [2,"always"],
    "space-unary-ops": 2,
    "strict": [2, "global"],
    "wrap-iife": [2, "inside"]
  }
}